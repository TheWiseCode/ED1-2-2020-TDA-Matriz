#include <iostream>
#include "UMatrizV.h"

using namespace std;

int main() {
	MatrizV* m = new MatrizV();
	m->dimensionar(5, 5);
	m->poner(1, 1, 1);
	m->poner(2, 2, 1);
	m->poner(3, 3, 1);
	m->poner(4, 4, 1);
	m->poner(5, 5, 1);
	m->poner(4, 4, 0);
	m->definfir_valor_repetido(1);
	cout << m->to_str() << endl;
	system("pause");
	return 0;
}
